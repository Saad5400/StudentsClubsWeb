@model Club

<div class="mt-3">

</div>
<h2 class="">
    @Model.Name
</h2>
<hr/>
<h5>
    ادارة النادي
</h5>
<div>
    <table class="table table-hover">
        <tr>
            <th>الاسم</th>
            <th>اسم المستخدم</th>
            <th>البريد</th>
        </tr>
        @foreach (var admin in Model.ClubAdmins.Select(cd => cd.Admin))
        {
            <tr>
                <td>@(admin.FirstName + " " + admin.LastName)</td>
                <td>@admin.DisplayUsername</td>
                <td>@admin.Email</td>
            </tr>
        }
    </table>
</div>
<h5>
    الاعضاء
</h5>
<div>
    <table class="table table-hover">
        <tr>
            <th>اسم المستخدم</th>
            <th>البريد العام</th>
        </tr>
        @foreach (var user in Model.Members)
        {
            <tr>
                <td>@user.DisplayUsername</td>
                <td>@user.SchoolEmail</td>
            </tr>
        }
    </table>
</div>